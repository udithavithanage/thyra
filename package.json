{
  "name": "thyra",
  "version": "1.1.1",
  "description": "Simple project folder opener CLI",
  "type": "module",
  "bin": {
    "thyra": "dist/cli.js"
  },
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir ./dist --target node --banner '#!/usr/bin/env node' --minify",
    "start": "node ./dist/cli.js",
    "lint": "eslint ."
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cli",
    "projects",
    "folders",
    "shortcut",
    "vscode"
  ],
  "author": "Uditha Vithanage",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/udithavithanage/thyra.git"
  },
  "bugs": {
    "url": "https://github.com/udithavithanage/thyra/issues"
  },
  "homepage": "https://github.com/udithavithanage/thyra#readme",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "bun": "^1.3.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "picocolors": "^1.1.1"
  }
}
